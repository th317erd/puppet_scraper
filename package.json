  "dependencies": {
    "inflection": "^1.13.1",
    "mythix": "^1.0.3",
    "nife": "^1.5.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "puppeteer": "^13.1.1",
    "sequelize": "^6.13.0",
